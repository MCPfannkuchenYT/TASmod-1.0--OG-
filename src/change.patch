+package net.tasmod.virtual;
+
+import java.util.LinkedList;
+import java.util.Queue;
+
+import org.lwjgl.input.Mouse;
+
+import net.tasmod.TASmod;
+
+public class VirtualMouse {
+
+	public static final class VirtualMouseEvent {
+		
+		/** The current position */
+		public int posX;
+
+		/** The current position */
+		public int posY;
+
+		/** MAC only, is the Cursor grabbed */
+		public boolean grabbed;
+		
+		/** Scroll wheel */
+		public int wheel;
+		
+		/** Left, Right, Middle etc..*/
+		public int eventButton;
+		
+		/* EventButton down */
+		public boolean eventState;
+		
+		public VirtualMouseEvent(final int i, final int j, final boolean b, final int k, final int l, final boolean c) {
+			this.posX = i;
+			this.posY = j;
+			this.grabbed = b;
