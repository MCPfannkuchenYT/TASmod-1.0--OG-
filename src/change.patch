+		public double posY;
+
+		/** MAC only, is the Cursor grabbed */
+		public boolean grabbed;
+		
+		/** Scroll wheel */
+		public int wheel;
+		
+		/** Left, Right, Middle etc..*/
+		public int eventButton;
+		
+		/* EventButton down */
+		public boolean eventState;
+		
+		public VirtualMouseEvent(final double i, final double j, final boolean b, final int k, final int l, final boolean c) {
+			this.posX = i;
+			this.posY = j;
+			this.grabbed = b;
+			this.wheel = k;
+			this.eventButton = l;
+			this.eventState = c;
+		}
+		
+		@Override
+		public final String toString() {
+			return posX + "!" + posY + "!" + grabbed + "!" + wheel + "!" + eventButton + "!" + eventState;
+		}
+		
+		public static final VirtualMouseEvent fromString(final String object) {
+			return new VirtualMouseEvent(Double.parseDouble(object.split("!")[0]), Double.parseDouble(object.split("!")[1]), Boolean.parseBoolean(object.split("!")[2]), Integer.parseInt(object.split("!")[3]), Integer.parseInt(object.split("!")[4]), Boolean.parseBoolean(object.split("!")[5]));
+		}
+		
+	}
+	
+	// TODO: Reimplement isButtonDown with a Virtual Mouse
+	
+	public final static Queue<VirtualMouseEvent> mouseEventsForTick = new LinkedList<>();
+	public static VirtualMouseEvent currentMouseEvents = new VirtualMouseEvent(0, 0, false, 0, 0, false);
+	public static boolean hack = false;
+	
+	public static boolean listen = false;
+	public static VirtualMouseEvent currentlyListening = new VirtualMouseEvent(-1, -1, false, -1, -1, false);
+	
+	public static boolean isButtonDown(final int i) {
+		if (!hack) {
+			boolean val = Mouse.isButtonDown(i);
+			if (listen) {
+				currentlyListening.eventButton = i;
+				currentlyListening.eventState = val;
+			}
+			return val;
+		}
+		for (final VirtualMouseEvent virtualMouseEvent : mouseEventsForTick) 
+			if (virtualMouseEvent.eventState == true && virtualMouseEvent.eventButton == i) return true;
+		return false;
+	}
+	
+	public static boolean next() {
+		if (listen) {
+			if (currentlyListening != null) {
+				TASmod.mouseTick(currentlyListening);
+				currentlyListening = new VirtualMouseEvent(currentlyListening.posX, currentlyListening.posY, false, 0, -1, false);
+			} else {
+				currentlyListening = new VirtualMouseEvent(-1, -1, false, 0, -1, false);
+			}
+		}
+		if (!hack) return Mouse.next();
+		if (mouseEventsForTick.isEmpty()) return false;
+		currentMouseEvents = mouseEventsForTick.poll();
+		return true;
+	}
+	
+	public static void setGrabbed(final boolean b) {
+		if (!hack) {
+			Mouse.setGrabbed(b);
+			if (listen) {
+				currentlyListening.grabbed = true;
